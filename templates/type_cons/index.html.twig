{% extends 'back.html.twig' %}

{% block title %}TypeCons index{% endblock %}

{% block body %}
    <h1>TypeCons index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_con in type_cons %}
            <tr>
                <td>{{ type_con.id }}</td>
                <td>{{ type_con.type }}</td>
                <td>
                    <a href="{{ path('app_type_cons_show', {'id': type_con.id}) }}">show</a>
                    <a href="{{ path('app_type_cons_edit', {'id': type_con.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_cons_new') }}">Create new</a>
{% endblock %}
